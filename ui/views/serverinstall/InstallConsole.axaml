<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SCLauncher.ui.views.serverinstall.InstallConsole">
		<Border Background="Black" BorderBrush="{StaticResource SystemAccentColor}"
		        BorderThickness="1" CornerRadius="5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
			<ScrollViewer Name="Scroller">
				<SelectableTextBlock DockPanel.Dock="Bottom" Margin="5"
				                     Name="Console"
				                     TextWrapping="Wrap" FontFamily="Consolas, Courier New, monospace"/>
			</ScrollViewer>
		</Border>
</UserControl>
